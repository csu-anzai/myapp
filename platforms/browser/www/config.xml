<?xml version='1.0' encoding='utf-8'?>
<widget id="me.digitalharbor.tantasc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Tanta Sporting Club</name>
    <description>
        The official mobile application of Tanta Sporting Club.
    </description>
    <author email="info@digitalharbor.me" href="http://digitalharbor.me">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-x-socialsharing" source="npm" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.4.1" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="android-build-tool" value="gradle" />
</widget>
